{
    "lingo": {
        "version": "page-beta-1"
    },
    "params": {
        "project_name": {
            "name": {
                "lower_case": "project name"
            },
            "type": "str",
            "default": "My Lingo Project",
            "description": "Name of the Lingo project"
        },
        "module_name": {
            "name": {
                "lower_case": "module name"
            },
            "type": "str",
            "default": "my-module-a",
            "description": "Name of the module"
        },
        "model_names": {
            "name": {
                "lower_case": "model names"
            },
            "type": "list",
            "default": ["my-model-a", "my-model-b"],
            "description": "List of model names in this project's modules",
            "item_type": "str"
        },
        "op_names": {
            "name": {
                "lower_case": "op names"
            },
            "type": "list",
            "default": ["my-op-a", "my-op-b"],
            "description": "List of operation names in this module",
            "item_type": "str"
        }
    },
    "state": {
        "header": {
            "type": "str",
            "calc": {
                "call": "join",
                "args": {
                    "separator": "",
                    "items": [
                        ":: ",
                        {
                            "params": {
                                "project_name": {}
                            }
                        },
                        " :: ",
                        {
                            "params": {
                                "module_name": {}
                            }
                        }
                    ]
                }
            }
        }
    },
    "ops": {},
    "output": [
        {
            "heading": {
                "state": {
                    "header": {}
                }
            },
            "level": 1
        },
        {
            "heading": ":: available models",
            "level": 2
        },
        {
            "break": 1
        },
        {
            "call": "map",
            "args": {
                "iterable": {
                    "params": {
                        "model_names": {}
                    }
                },
                "function": {
                    "link": {
                        "call": "concat",
                        "args": {
                            "items": [
                                "/",
                                {
                                    "self": "item"
                                }
                            ]
                        }
                    },
                    "text": {
                        "self": "item"
                    }
                }
            }
        },
        {
            "heading": ":: available operations",
            "level": 2
        },
        {
            "break": 1
        },
        {
            "call": "map",
            "args": {
                "iterable": {
                    "params": {
                        "op_names": {}
                    }
                },
                "function": {
                    "link": {
                        "call": "concat",
                        "args": {
                            "items": [
                                "/",
                                {
                                    "self": "item"
                                }
                            ]
                        }
                    },
                    "text": {
                        "self": "item"
                    }
                }
            }
        }
    ]
}